<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup Label="Configuration" Condition="'$(TargetFramework)'=='net461'">
    <OutputType>exe</OutputType>
  </PropertyGroup>
  <PropertyGroup>
    <TargetFrameworks>net461;netcoreapp2.0</TargetFrameworks>
    <Authors>Sergey Savchuk</Authors>
    <Company />
    <Product />
    <Description>Tool for generating SSL certificates</Description>
    <RepositoryUrl>https://github.com/SavchukSergey/easySSL</RepositoryUrl>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <ApplicationIcon />
    <OutputTypeEx>exe</OutputTypeEx>
    <StartupObject />
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Asn1" Version="1.0.8" />
    <!-- <PackageReference Include="System.Security.Cryptography.Csp" Version="4.3.0" /> -->
  </ItemGroup>
</Project>